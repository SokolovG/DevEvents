[flake8]
max-line-length = 120
exclude =
    .git,
    __pycache__,
    */migrations/*,
    venv,
    env

ignore =
    E203,  # whitespace before ':'
    E266,  # too many leading '#' for block comment
    W503,   # line break before binary operator
    D100,
    D101,
    D104,
    D106


max-complexity = 10